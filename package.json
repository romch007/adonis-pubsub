{
  "name": "adonis-pubsub",
  "version": "1.0.0",
  "description": "PubSub for adonis",
  "main": "build/providers/PubSubProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src",
    "build/config.d.ts",
    "build/config.js"
  ],
  "scripts": {
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np --message=\"chore(release): %s\"",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json romch007/adonis-pubsub",
    "prepare": "husky install"
  },
  "typings": "./build/adonis-typings/index.d.ts",
  "keywords": [
    "adonis",
    "pubsub",
    "mqtt"
  ],
  "author": "Romain Chardiny <romain.chardiny@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/application": "^5.2.5",
    "@adonisjs/core": "^5.1.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/require-ts": "^2.0.12",
    "@adonisjs/sink": "^5.3.2",
    "@types/node": "^18.0.0",
    "async-mqtt": "^2.6.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "github-label-sync": "^2.2.0",
    "husky": "^8.0.1",
    "mrm": "^4.0.0",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@poppinss/manager": "^5.0.2"
  },
  "peerDependencies": {
    "@adonisjs/application": "^5.2.5",
    "@adonisjs/core": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romch007/adonis-pubsub.git"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [
      "github-actions"
    ],
    "minNodeVersion": "16.13.1",
    "probotApps": [],
    "runGhActionsOnWindows": true
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  }
}
